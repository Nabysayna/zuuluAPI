<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Patterns" table="patterns">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sizemin" type="integer" column="sizemin" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sizemax" type="integer" column="sizemax" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="servicename" type="string" column="servicename" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="servicedesignation" type="string" column="servicedesignation" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
