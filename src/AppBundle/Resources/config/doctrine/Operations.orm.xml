<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Operations" table="operations">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="iduser" type="integer" column="iduser" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="infoop" type="integer" column="infoop" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateoperation" type="integer" column="dateoperation" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
